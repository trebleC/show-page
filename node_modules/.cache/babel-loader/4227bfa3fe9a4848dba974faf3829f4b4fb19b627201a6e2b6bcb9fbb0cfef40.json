{"ast":null,"code":"import { ref, reactive, onMounted } from 'vue';\nimport { ElCarousel, ElCarouselItem } from 'element-plus';\nexport default {\n  name: 'PostList',\n  components: {\n    ElCarousel,\n    ElCarouselItem\n  },\n  setup(props, ctx) {\n    let carouselBox = ref(null);\n    let carouselHeight = ref('400px');\n    let carouselList = reactive([{\n      imageUrl: 'https://my903-1256859760.cos.ap-guangzhou.myqcloud.com/logo/logo1.png',\n      url: 'https://www.baidu.com/'\n    }, {\n      imageUrl: 'https://my903-1256859760.cos.ap-guangzhou.myqcloud.com/logo/logo1.png',\n      url: 'https://www.baidu.com/'\n    }]);\n    onMounted(() => {\n      console.log('carouselBox', carouselBox.value.offsetWidth);\n      carouselHeight.value = parseInt(6 * carouselBox.value.offsetWidth / 16) + 'px';\n      console.log('carouselHeight', carouselHeight);\n    });\n    const jumpPage = item => {\n      window.open(item.url);\n    };\n    return {\n      carouselBox,\n      carouselHeight,\n      carouselList,\n      jumpPage\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","onMounted","ElCarousel","ElCarouselItem","name","components","setup","props","ctx","carouselBox","carouselHeight","carouselList","imageUrl","url","console","log","value","offsetWidth","parseInt","jumpPage","item","window","open"],"sources":["/Library/myapp/product-shop/src/views/Home/Home.vue"],"sourcesContent":["<template>\n    <div class=\"home\">\n        <div class=\"carousel-box\" ref=\"carouselBox\">\n            <el-carousel trigger=\"click\" :height=\"carouselHeight\">\n            <el-carousel-item v-for=\"item in carouselList\" :key=\"item\">\n                <img class=\"carousel-img\" :src=\"item.imageUrl\" alt=\"\" srcset=\"\" @click=\"jumpPage(item)\">\n            </el-carousel-item>\n        </el-carousel>\n        </div>\n\n    </div>\n</template>\n    \n<script>\nimport { ref, reactive, onMounted } from 'vue'\nimport { ElCarousel, ElCarouselItem } from 'element-plus'\nexport default {\n    name: 'PostList',\n    components: { ElCarousel, ElCarouselItem },\n    setup(props, ctx) {\n        let carouselBox = ref(null)\n        let carouselHeight = ref('400px')\n        let carouselList = reactive([{\n            imageUrl:'https://my903-1256859760.cos.ap-guangzhou.myqcloud.com/logo/logo1.png',\n            url:'https://www.baidu.com/'\n        },\n        {\n            imageUrl:'https://my903-1256859760.cos.ap-guangzhou.myqcloud.com/logo/logo1.png',\n            url:'https://www.baidu.com/'\n        }\n        ])\n        onMounted(()=>{\n            console.log('carouselBox',carouselBox.value.offsetWidth);\n            \n            carouselHeight.value = parseInt(6 * carouselBox.value.offsetWidth / 16) + 'px'\n            console.log('carouselHeight',carouselHeight);\n            \n            \n        })\n        const jumpPage = (item)=>{\n            window.open(item.url)\n        }\n        return {\n            carouselBox,\n            carouselHeight,\n            carouselList,\n            jumpPage\n        }\n    }\n}\n</script>\n    \n<style lang=\"less\">\n.home {\n    width: 100%;\n    overflow: hidden;\n    margin-left: auto;\n    margin-right: auto;\n\n    max-width: 100%;\n    max-height: 720px;\n\n    .carousel-box {\n        .carousel-img{\n            width: 100%;\n            object-fit: cover;\n            cursor: pointer;\n        }\n    }\n\n    .el-carousel__item h3 {\n        color: #475669;\n        opacity: 0.75;\n        line-height: 150px;\n        margin: 0;\n        text-align: center;\n    }\n\n    .el-carousel__item:nth-child(2n) {\n        background-color: #99a9bf;\n    }\n\n    .el-carousel__item:nth-child(2n + 1) {\n        background-color: #d3dce6;\n    }\n}\n</style>"],"mappings":"AAcA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,KAAI;AAC7C,SAASC,UAAU,EAAEC,cAAa,QAAS,cAAa;AACxD,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IAAEH,UAAU;IAAEC;EAAe,CAAC;EAC1CG,KAAKA,CAACC,KAAK,EAAEC,GAAG,EAAE;IACd,IAAIC,W