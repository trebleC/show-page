{"ast":null,"code":"import { ref, onMounted, onUnmounted } from 'vue';\nimport { getTab } from '@/api/home';\nexport default {\n  name: 'SideBar',\n  setup(props, ctx) {\n    const navKey = ref('');\n    const navList = ref([[{\n      name: '待办',\n      key: 'todo'\n    }], [{\n      name: '登录',\n      key: 'login'\n    }, {\n      name: '设置',\n      key: 'setting'\n    }, {\n      name: '关于',\n      key: 'about'\n    }]]);\n    const onNav = nav => {\n      navKey.value = nav.key;\n      ctx.emit('navChang', nav.key);\n    };\n    getTab().then(res => {\n      navList.value.unshift(res.data);\n      onNav(navList.value[0][0]);\n    });\n    return {\n      navKey,\n      navList,\n      onNav\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","getTab","name","setup","props","ctx","navKey","navList","key","onNav","nav","value","emit","then","res","unshift","data"],"sources":["/Library/myapp/product-shop/src/views/Layout/SideBar.vue"],"sourcesContent":["<template>\n    <div class=\"side-bar\">\n\n        <div class=\"nav-box\">\n            <div class=\"logo\">\n                <img src=\"@/assets/logo.png\" alt=\"\">\n            </div>\n            <div v-for=\"(navItem, index) in navList\" :class=\"{ 'nav-border': index!=navList.length-1 }\">\n                <div v-for=\"(nav, navIndex) in navItem\" :key=\"'nav' + index + '_' + navIndex\" @click=\"onNav(nav)\"\n                    :class=\"['nav', { active: nav.key == navKey }]\">\n                    <div >{{ nav.name }}</div>\n                </div>\n            </div>\n\n        </div>\n        <div class=\"version\">\n            <div class=\"name\">AirTab</div>\n            <div class=\"desc\">v1.0</div>\n        </div>\n    </div>\n</template>\n<script>\nimport { ref, onMounted, onUnmounted } from 'vue'\nimport {getTab} from '@/api/home'\nexport default {\n    name: 'SideBar',\n    setup(props,ctx) {\n        const navKey = ref('')\n        const navList = ref([[\n            {\n                name: '待办',\n                key: 'todo',\n            },\n        ],\n        [\n            {\n                name: '登录',\n                key: 'login',\n            },\n            {\n                name: '设置',\n                key: 'setting',\n            },\n            {\n                name: '关于',\n                key: 'about',\n            },\n        ]])\n        const onNav = (nav) => {\n            navKey.value = nav.key\n            ctx.emit('navChang',nav.key)\n        }\n\n        getTab().then(res => {\n            navList.value.unshift(res.data)\n            onNav(navList.value[0][0])\n        })\n\n        return {\n            navKey,\n            navList,\n            onNav\n        }\n    }\n}\n</script>\n<style lang=\"less\">\n.side-bar {\n    padding: 20px 0 60px 0;\n    box-sizing: border-box;\n    width: 90px;\n    height: 100%;\n    background-color: #fff;\n    display: flex;\n    justify-content: space-between;\n    flex-direction: column;\n\n    .logo {\n\n        display: flex;\n        justify-content: center;\n\n        img {\n            width: 80px;\n            display: inline-block;\n        }\n    }\n\n    .nav-box {\n        padding: 10px;\n        box-sizing: border-box;\n\n        .nav-border {\n            border-bottom: 1px solid #ececec;\n            margin-bottom: 10px;\n        }\n\n        .nav {\n            width: 100%;\n            padding: 14px 0;\n            box-sizing: border-box;\n            border-radius: 5px;\n            cursor: pointer;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            font-size: 14px;\n            color: #333;\n            margin: 10px 0;\n\n            &:hover {\n                color: #000;\n                background-color: rgba(236, 236, 236, .3);\n            }\n        }\n\n        .active {\n            color: #3880ff;\n            font-weight: bold;\n            background-color: rgba(49, 113, 224, .08);\n\n            &:hover {\n                color: #3880ff;\n                background-color: rgba(49, 113, 224, .08);\n            }\n        }\n    }\n    .version{\n        text-align: center;\n        .name{\n            color: #3880ff;\n            font-size: 14px;\n        }\n        .desc{\n            color: #666;\n            font-size: 12px;\n        }\n\n    }\n}</style>"],"mappings":"AAsBA,SAASA,GAAG,EAAEC,SAAS,EAAEC,WAAU,QAAS,KAAI;AAChD,SAAQC,MAAM,QAAO,YAAW;AAChC,eAAe;EACXC,IAAI,EAAE,SAAS;EACfC,KAAKA,CAACC,KAAK,EAACC,GAAG,EAAE;IACb,MAAMC,MAAK,GAAIR,GAAG,CAAC,EAAE;IACrB,MAAMS,OAAM,GAAIT,GAAG,CAAC,CAAC,CACjB;MACII,IAAI,EAAE,IAAI;MACVM,GAAG,EAAE;IACT,CAAC,CACJ,EACD,CACI;MACIN,IAAI,EAAE,IAAI;MACVM,GAAG,EAAE;IACT,CAAC,EACD;MACIN,IAAI,EAAE,IAAI;MACVM,GAAG,EAAE;IACT,CAAC,EACD;MACIN,IAAI,EAAE,IAAI;MACVM,GAAG,EAAE;IACT,CAAC,CACJ,CAAC;IACF,MAAMC,KAAI,GAAKC,GAAG,IAAK;MACnBJ,MAAM,CAACK,KAAI,GAAID,GAAG,CAACF,GAAE;MACrBH,GAAG,CAACO,IAAI,CAAC,UAAU,EAACF,GAAG,CAACF,GAAG;IAC/B;IAEAP,MAAM,CAAC,CAAC,CAACY,IAAI,CAACC,GAAE,IAAK;MACjBP,OAAO,CAACI,KAAK,CAACI,OAAO,CAACD,GAAG,CAACE,IAAI;MAC9BP,KAAK,CAACF,OAAO,CAACI,KAAK,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IAC7B,CAAC;IAED,OAAO;MACHL,MAAM;MACNC,OAAO;MACPE;IACJ;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}