{"ast":null,"code":"import useClipboard from 'vue-clipboard3';\nimport { getSongList } from '@/api/poster';\nimport { ref, reactive } from 'vue';\nimport { ElMessage } from 'element-plus';\nexport default {\n  name: 'PostList',\n  setup(props, ctx) {\n    const {\n      toClipboard\n    } = useClipboard();\n    const copyName = async name => {\n      await toClipboard(name);\n      ElMessage({\n        message: '已复制',\n        type: 'success'\n      });\n    };\n    const dialogFormVisible = ref(false);\n    const formLabelWidth = '140px';\n    const form = reactive({\n      name: '',\n      region: '',\n      date1: '',\n      date2: '',\n      delivery: false,\n      type: [],\n      resource: '',\n      desc: ''\n    });\n    const gridData = [{\n      date: '2016-05-02',\n      name: 'John Smith',\n      address: 'No.1518,  Jinshajiang Road, Putuo District'\n    }, {\n      date: '2016-05-04',\n      name: 'John Smith',\n      address: 'No.1518,  Jinshajiang Road, Putuo District'\n    }, {\n      date: '2016-05-01',\n      name: 'John Smith',\n      address: 'No.1518,  Jinshajiang Road, Putuo District'\n    }, {\n      date: '2016-05-03',\n      name: 'John Smith',\n      address: 'No.1518,  Jinshajiang Road, Putuo District'\n    }];\n    let songList = ref([]);\n    getSongList().then(res => {\n      console.log(res);\n      songList.value = res.data;\n    });\n    const showDetail = item => {\n      dialogFormVisible.value = true;\n    };\n    return {\n      copyName,\n      songList,\n      dialogFormVisible,\n      formLabelWidth,\n      form,\n      gridData,\n      showDetail\n    };\n  }\n};","map":{"version":3,"names":["useClipboard","getSongList","ref","reactive","ElMessage","name","setup","props","ctx","toClipboard","copyName","message","type","dialogFormVisible","formLabelWidth","form","region","date1","date2","delivery","resource","desc","gridData","date","address","songList","then","res","console","log","value","data","showDetail","item"],"sources":["/Library/myapp/product-shop/src/views/Poster/PosterList.vue"],"sourcesContent":["<template>\n    <div class=\"post-list\">\n        <div class=\"filter\">搜索</div>\n        <div class=\"list-box\">\n            <div class=\"list\" v-for=\"(item, index) in songList\" :key=\"index\">\n                <img class=\"post-img\" @click=\"showDetail(item)\" :src=\"item.thumbnail_image_url\" alt=\"\" srcset=\"\">\n                <div class=\"name\" @click=\"copyName(item.song_name)\">{{ item.song_name }}</div>\n                <div class=\"singer desc\" @click=\"\">{{ item.singer }}</div>\n                <div class=\"desc\">{{ item.publish_at }}</div>\n            </div>\n        </div>\n\n        <el-dialog v-model=\"dialogFormVisible\" title=\"Shipping address\">\n            <el-form :model=\"form\">\n                <el-form-item label=\"Promotion name\" :label-width=\"formLabelWidth\">\n                    <el-input v-model=\"form.name\" autocomplete=\"off\" />\n                </el-form-item>\n                <el-form-item label=\"Zones\" :label-width=\"formLabelWidth\">\n                    <el-select v-model=\"form.region\" placeholder=\"Please select a zone\">\n                        <el-option label=\"Zone No.1\" value=\"shanghai\" />\n                        <el-option label=\"Zone No.2\" value=\"beijing\" />\n                    </el-select>\n                </el-form-item>\n            </el-form>\n            <template #footer>\n                <span class=\"dialog-footer\">\n                    <el-button @click=\"dialogFormVisible = false\">Cancel</el-button>\n                    <el-button type=\"primary\" @click=\"dialogFormVisible = false\">\n                        Confirm\n                    </el-button>\n                </span>\n            </template>\n        </el-dialog>\n    </div>\n</template>\n    \n<script>\nimport useClipboard from 'vue-clipboard3'\nimport { getSongList } from '@/api/poster'\nimport { ref,reactive } from 'vue'\nimport { ElMessage } from 'element-plus'\nexport default {\n    name: 'PostList',\n    setup(props, ctx) {\n        const { toClipboard } = useClipboard()\n        const copyName = async (name) => {\n            await toClipboard(name)\n            ElMessage({\n                message: '已复制',\n                type: 'success',\n            })\n        }\n\n\n\n        const dialogFormVisible = ref(false)\n        const formLabelWidth = '140px'\n\n        const form = reactive({\n            name: '',\n            region: '',\n            date1: '',\n            date2: '',\n            delivery: false,\n            type: [],\n            resource: '',\n            desc: '',\n        })\n\n        const gridData = [\n            {\n                date: '2016-05-02',\n                name: 'John Smith',\n                address: 'No.1518,  Jinshajiang Road, Putuo District',\n            },\n            {\n                date: '2016-05-04',\n                name: 'John Smith',\n                address: 'No.1518,  Jinshajiang Road, Putuo District',\n            },\n            {\n                date: '2016-05-01',\n                name: 'John Smith',\n                address: 'No.1518,  Jinshajiang Road, Putuo District',\n            },\n            {\n                date: '2016-05-03',\n                name: 'John Smith',\n                address: 'No.1518,  Jinshajiang Road, Putuo District',\n            },\n        ]\n\n\n\n\n        let songList = ref([])\n        getSongList().then(res => {\n            console.log(res)\n            songList.value = res.data\n        })\n\n        const showDetail = (item)=>{\n            dialogFormVisible.value = true\n        }\n        return {\n            copyName,\n            songList,\n            dialogFormVisible,\n            formLabelWidth,\n            form,\n            gridData,\n            showDetail\n        }\n    }\n}\n</script>\n    \n<style lang=\"less\">\n.post-list {\n    // display: flex;\n    // justify-content: center;\n    position: relative;\n    padding: 16px;\n    box-sizing: border-box;\n    width: 100%;\n    height: 100%;\n\n\n    .list-box {\n        margin: 0 auto;\n        display: flex;\n        flex-wrap: wrap;\n        background-color: #fff;\n        padding: 40px;\n        box-sizing: border-box;\n        border-radius: 4px;\n        width: 1100px;\n\n        .list {\n            width: 234px;\n            margin-bottom: 20px;\n            margin-right: 28px;\n            box-sizing: border-box;\n\n            &:nth-child(4n) {\n                margin-right: 0;\n            }\n        }\n    }\n\n    .post-img {\n        width: 100%;\n        height: 234px;\n        object-fit: cover;\n        border-radius: 3px;\n        transition: all .5s;\n        cursor: pointer;\n\n        &:hover {\n            transform: scale(1.05);\n        }\n    }\n\n    .name {\n        color: #000;\n        font-size: 14px;\n        cursor: pointer;\n    }\n\n    .singer {\n        cursor: pointer;\n    }\n\n    .desc {\n        color: #999;\n        font-size: 14px;\n    }\n}\n</style>"],"mappings":"AAqCA,OAAOA,YAAW,MAAO,gBAAe;AACxC,SAASC,WAAU,QAAS,cAAa;AACzC,SAASC,GAAG,EAACC,QAAO,QAAS,KAAI;AACjC,SAASC,SAAQ,QAAS,cAAa;AACvC,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,KAAKA,CAACC,KAAK,EAAEC,GAAG,EAAE;IACd,MAAM;MAAEC;IAAY,IAAIT,YAAY,CAAC;IACrC,MAAMU,QAAO,GAAI,MAAOL,IAAI,IAAK;MAC7B,MAAMI,WAAW,CAACJ,IAAI;MACtBD,SAAS,CAAC;QACNO,OAAO,EAAE,KAAK;QACdC,IAAI,EAAE;MACV,CAAC;IACL;IAIA,MAAMC,iBAAgB,GAAIX,GAAG,CAAC,KAAK;IACnC,MAAMY,cAAa,GAAI,OAAM;IAE7B,MAAMC,IAAG,GAAIZ,QAAQ,CAAC;MAClBE,IAAI,EAAE,EAAE;MACRW,MAAM,EAAE,EAAE;MACVC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,KAAK;MACfP,IAAI,EAAE,EAAE;MACRQ,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE;IACV,CAAC;IAED,MAAMC,QAAO,GAAI,CACb;MACIC,IAAI,EAAE,YAAY;MAClBlB,IAAI,EAAE,YAAY;MAClBmB,OAAO,EAAE;IACb,CAAC,EACD;MACID,IAAI,EAAE,YAAY;MAClBlB,IAAI,EAAE,YAAY;MAClBmB,OAAO,EAAE;IACb,CAAC,EACD;MACID,IAAI,EAAE,YAAY;MAClBlB,IAAI,EAAE,YAAY;MAClBmB,OAAO,EAAE;IACb,CAAC,EACD;MACI